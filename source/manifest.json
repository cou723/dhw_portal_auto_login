{
	"manifest_version": 3,
	"name": "Awesome Extension",
	"version": "0.0.0",
	"description": "Turns the page red when you click the icon",
	"content_scripts": [
		{ "matches": ["https://portal.dhw.ac.jp/*/*"], "js": ["auto-login.js"] }
	],
	"icons": {
		"128": "icon.png"
	},
	"permissions": ["storage"],
	"host_permissions": ["https://portal.dhw.ac.jp/*/*"],
	"options_ui": {
		"browser_style": true,
		"page": "options.html"
	},
	"background": {
		"service_worker": "background.js",
		"type": "module"
	}
}
